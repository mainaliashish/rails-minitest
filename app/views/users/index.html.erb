
<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
  <% end %>
</div>

<% if !current_user %>
  <%= link_to "New user", new_user_path %>
<% else %>
  <%= link_to "All articles", articles_path %>
<% end%>
